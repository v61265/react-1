{
  "extends": ["plugin:react/jsx-runtime", "plugin:prettier/recommended"],
  "plugins": ["react", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "error",
    "react/jsx-uses-vars": "error"
  }
}
